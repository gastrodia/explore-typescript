{
    "version": "1.6.0-dev",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "outDir": "./build"
    },
    "filesGlob": [
        "./src/**/*.ts"
    ],
    "files": [
        "./src/a.ts",
        "./src/b.ts",
        "./src/ioc.ts"
    ]
}
